{
  "version": 2,
  "name": "dashboard2019",
  "builds": [
    { "src": "/dist/apps/dashboard2019/*", "use": "@now/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/dist/apps/dashboard2019/assets/$1"},
    { "src": "/(.*).(js|css|ico)", "dest": "/dist/apps/dashboard2019/$1.$2" },
    { "src": "/(.*)", "dest": "/dist/apps/dashboard2019/index.html" }
  ]
}

